<%= simple_form_for([:admin,@person]) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.association :team %>
    <%= f.input :photo, as: :file %>
    <%= f.input_field :about,{'id' => 'text'} %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
